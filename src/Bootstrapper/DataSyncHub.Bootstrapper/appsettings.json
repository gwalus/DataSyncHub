{
  "ApplicationName": "DataSyncHub",
  "Serilog": {
    "MinimumLevel": {
      "Defalut": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Secrets": {
    "ApiNinjasToken": ""
  },
  "MongoDB": {
    "ConnectionURI": "mongodb://mongodb:27017",
    "DatabaseName": "DataSyncHub",
    "CollectionName": "users"
  },
  "ElasticConfiguration": {
    "Uri": "elasticsearch:9200"
  },
  "Redis": {
    "Host": "redis",
    "Port": "6379"
  }
}